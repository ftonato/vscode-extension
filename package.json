{
  "name": "xata",
  "displayName": "xata-vscode-extension",
  "description": "Managed your xatabase from VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:xata.login",
    "onCommand:xata.logout",
    "onCommand:xata.refresh",
    "onView:xataExplorer"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Xata",
      "properties": {
        "xata.baseUrl": {
          "type": "string",
          "default": "https://xata.sh",
          "description": "Xata API base url"
        },
        "xata.appBaseUrl": {
          "type": "string",
          "default": "https://app.xata.io",
          "description": "Xata UI base url"
        },
        "xata.hideBranchLevel": {
          "type": "boolean",
          "default": "true",
          "description": "Hide branch level when a database have only one branch"
        },
        "xata.enableDatabaseColor": {
          "type": "boolean",
          "default": "true",
          "description": "Use database color for the icon"
        }
      }
    },
    "colors": [
      {
        "id": "xata.gray",
        "description": "Xata gray",
        "defaults": {
          "dark": "#CBD5E0",
          "highContrast": "#E2E8F0",
          "highContrastLight": "#4A5568",
          "light": "#A0AEC0"
        }
      },
      {
        "id": "xata.orange",
        "description": "Xata orange",
        "defaults": {
          "dark": "#F6AD55",
          "highContrast": "#FBD38D",
          "highContrastLight": "#C05621",
          "light": "#ED8936"
        }
      },
      {
        "id": "xata.green",
        "description": "Xata green",
        "defaults": {
          "dark": "#68D391",
          "highContrast": "#9AE6B4",
          "highContrastLight": "#2F855A",
          "light": "#48BB78"
        }
      },
      {
        "id": "xata.blue",
        "description": "Xata blue",
        "defaults": {
          "dark": "#63b3ed",
          "highContrast": "#90cdf4",
          "highContrastLight": "#2b6cb0",
          "light": "#4299e1"
        }
      },
      {
        "id": "xata.cyan",
        "description": "Xata cyan",
        "defaults": {
          "dark": "#76E4F7",
          "highContrast": "#9DECF9",
          "highContrastLight": "#00A3C4",
          "light": "#0BC5EA"
        }
      },
      {
        "id": "xata.purple",
        "description": "Xata purple",
        "defaults": {
          "dark": "#B794F4",
          "highContrast": "#D6BCFA",
          "highContrastLight": "#6B46C1",
          "light": "#9F7AEA"
        }
      },
      {
        "id": "xata.pink",
        "description": "Xata pink",
        "defaults": {
          "dark": "#F687B3",
          "highContrast": "#FBB6CE",
          "highContrastLight": "#B83280",
          "light": "#ED64A6"
        }
      }
    ],
    "commands": [
      {
        "command": "xata.login",
        "title": "Xata: Login"
      },
      {
        "command": "xata.logout",
        "title": "Xata: Logout"
      },
      {
        "command": "xata.insertRecord",
        "title": "Xata: Insert record"
      },
      {
        "command": "xata.refresh",
        "title": "Xata: Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "xata.addColumn",
        "title": "Add column",
        "icon": "$(add)"
      },
      {
        "command": "xata.deleteColumn",
        "title": "Delete column",
        "icon": "$(trash)"
      },
      {
        "command": "xata.renameColumn",
        "title": "Rename column",
        "icon": "$(edit)"
      },
      {
        "command": "xata.addWorkspace",
        "title": "Add workspace",
        "icon": "$(add)"
      },
      {
        "command": "xata.renameWorkspace",
        "title": "Rename workspace",
        "icon": "$(edit)"
      },
      {
        "command": "xata.addDatabase",
        "title": "Add database",
        "icon": "$(add)"
      },
      {
        "command": "xata.deleteWorkspace",
        "title": "Delete workspace",
        "icon": "$(trash)"
      },
      {
        "command": "xata.deleteDatabase",
        "title": "Delete database",
        "icon": "$(trash)"
      },
      {
        "command": "xata.deleteTable",
        "title": "Delete table",
        "icon": "$(trash)"
      },
      {
        "command": "xata.addBranch",
        "title": "Add branch",
        "icon": "$(git-pull-request-create)"
      },
      {
        "command": "xata.deleteBranch",
        "title": "Delete branch",
        "icon": "$(trash)"
      },
      {
        "command": "xata.addTable",
        "title": "Add table",
        "icon": "$(empty-window)"
      },
      {
        "command": "xata.previewData",
        "title": "Preview data",
        "icon": "$(eye)"
      },
      {
        "command": "xata.renameTable",
        "title": "Rename table",
        "icon": "$(edit)"
      },
      {
        "command": "xata.openInsertRecordTemplate",
        "title": "Insert record",
        "icon": "$(json)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "xata",
          "title": "Xata",
          "icon": "media/xata-icon.svg"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "xataExplorer",
        "contents": "Welcome to xata!\n[Get started](command:xata.login)",
        "when": "!xata.isLogged"
      },
      {
        "view": "xataExplorer",
        "contents": "It appears you donâ€™t have any workspaces.\nCreate a workspace to get started.\n[Create workspace](command:xata.addWorkspace)",
        "when": "xata.isLogged && xata.workspaceCount == 0"
      },
      {
        "view": "xataExplorer",
        "contents": "You are offline\n[Refresh](command:xata.refresh)",
        "when": "xata.isOffline"
      }
    ],
    "views": {
      "xata": [
        {
          "id": "xataExplorer",
          "name": "Xata",
          "icon": "media/xata-icon.svg"
        }
      ]
    },
    "menus": {
      "commandPalette": [
        {
          "command": "xata.login",
          "when": "!xata.isLogged"
        },
        {
          "command": "xata.logout",
          "when": "xata.isLogged"
        },
        {
          "command": "xata.addColumn",
          "when": "false"
        },
        {
          "command": "xata.deleteColumn",
          "when": "false"
        },
        {
          "command": "xata.renameColumn",
          "when": "false"
        },
        {
          "command": "xata.addDatabase",
          "when": "false"
        },
        {
          "command": "xata.deleteDatabase",
          "when": "false"
        },
        {
          "command": "xata.deleteWorkspace",
          "when": "false"
        },
        {
          "command": "xata.renameWorkspace",
          "when": "false"
        },
        {
          "command": "xata.addWorkspace",
          "when": "false"
        },
        {
          "command": "xata.addBranch",
          "when": "false"
        },
        {
          "command": "xata.deleteBranch",
          "when": "false"
        },
        {
          "command": "xata.addTable",
          "when": "false"
        },
        {
          "command": "xata.deleteTable",
          "when": "false"
        },
        {
          "command": "xata.renameTable",
          "when": "false"
        },
        {
          "command": "xata.previewData",
          "when": "false"
        },
        {
          "command": "xata.openInsertRecordTemplate",
          "when": "false"
        }
      ],
      "view/title": [
        {
          "command": "xata.refresh",
          "when": "view == xataExplorer && xata.isLogged",
          "group": "navigation"
        },
        {
          "command": "xata.addWorkspace",
          "when": "view == xataExplorer && xata.isLogged"
        },
        {
          "command": "xata.logout",
          "when": "view == xataExplorer && xata.isLogged"
        },
        {
          "command": "xata.login",
          "when": "view == xataExplorer && !xata.isLogged"
        }
      ],
      "view/item/context": [
        {
          "command": "xata.addColumn",
          "when": "view == xataExplorer && viewItem == table",
          "group": "inline"
        },
        {
          "command": "xata.deleteColumn",
          "when": "view == xataExplorer && viewItem == column"
        },
        {
          "command": "xata.renameColumn",
          "when": "view == xataExplorer && viewItem == column"
        },
        {
          "command": "xata.deleteWorkspace",
          "when": "view == xataExplorer && viewItem == workspace"
        },
        {
          "command": "xata.renameWorkspace",
          "when": "view == xataExplorer && viewItem == workspace"
        },
        {
          "command": "xata.addDatabase",
          "when": "view == xataExplorer && viewItem == workspace",
          "group": "inline"
        },
        {
          "command": "xata.deleteDatabase",
          "when": "view == xataExplorer && viewItem == database"
        },
        {
          "command": "xata.deleteDatabase",
          "when": "view == xataExplorer && viewItem == oneBranchDatabase"
        },
        {
          "command": "xata.addBranch",
          "when": "view == xataExplorer && viewItem == database",
          "group": "inline"
        },
        {
          "command": "xata.deleteBranch",
          "when": "view == xataExplorer && viewItem == branch"
        },
        {
          "command": "xata.addBranch",
          "when": "view == xataExplorer && viewItem == oneBranchDatabase",
          "group": "inline"
        },
        {
          "command": "xata.addTable",
          "when": "view == xataExplorer && viewItem == oneBranchDatabase",
          "group": "inline"
        },
        {
          "command": "xata.renameTable",
          "when": "view == xataExplorer && viewItem == table",
          "group": "1_actions"
        },
        {
          "command": "xata.addTable",
          "when": "view == xataExplorer && viewItem == branch",
          "group": "inline"
        },
        {
          "command": "xata.deleteTable",
          "when": "view == xataExplorer && viewItem == table",
          "group": "1_actions"
        },
        {
          "command": "xata.previewData",
          "when": "view == xataExplorer && viewItem == table",
          "group": "inline"
        },
        {
          "command": "xata.openInsertRecordTemplate",
          "when": "view == xataExplorer && viewItem == table",
          "group": "5_templates"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "test": "vitest",
    "gen": "openapi-codegen gen xata"
  },
  "devDependencies": {
    "@openapi-codegen/cli": "^1.5.0",
    "@openapi-codegen/typescript": "^3.0.1",
    "@types/flat": "^5.0.2",
    "@types/json-schema": "^7.0.11",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "typescript": "^4.5.5",
    "vitest": "^0.9.3"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.29",
    "@vscode/webview-ui-toolkit": "^1.0.0",
    "date-fns": "^2.28.0",
    "flat": "^5.0.2",
    "jsonc": "^2.0.0",
    "undici": "^5.0.0"
  }
}
